-->

<class 
    name="com.card.model.BatchProduct" 
    table="BATCH_PRODUCT"
>
	<cache usage="read-write" /> 
    <id
        name="batchProductId"
        type="java.lang.Integer"
        column="BATCH_PRODUCT_ID"
    >
    
        <generator class="identity"/>
    </id>

    <property
        name="protocolCode"
        type="java.lang.String"
        column="PROTOCOL_CODE"
    />
    <property
        name="batchCode"
        type="java.lang.String"
        column="BATCH_CODE"
    />
    <property
        name="productId"
        type="java.lang.Integer"
        column="PRODUCT_ID"
    />
</class>
</hibernate-mapping>