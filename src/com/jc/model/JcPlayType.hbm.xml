<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.jc.model.JcPlayType" table="jc_play_type">
		<cache usage="read-write" />
		<id name="pTId" type="java.lang.Integer" column="p_t_id">
			<generator class="identity" />
		</id>
       <property name="fId" type="java.lang.Integer" column="f_id" length="11" />
       <property name="title" type="java.lang.String" column="title" length="50"/>
       <property name="status" type="java.lang.String" column="status" length="1" />
       <property name="openResult" type="java.lang.String" column="open_result" length="50"/>
       <property name="openStatus" type="java.lang.String" column="open_status" length="1" />
	</class>
</hibernate-mapping>