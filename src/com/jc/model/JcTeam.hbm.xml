<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.jc.model.JcTeam" table="jc_team">
		<cache usage="read-write" />
		<id name="Id" type="java.lang.Integer" column="id">
			<generator class="identity" />
		</id>
       <property name="tId1" type="java.lang.Integer" column="t_id1" length="11" />
       <property name="tId2" type="java.lang.Integer" column="t_id2" length="11" />
       <property name="title" type="java.lang.String" column="title" length="50"/>
       <property name="img" type="java.lang.String" column="img" length="200"/>
       <property name="remarks" type="java.lang.String" column="remarks" length="200"/>
	</class>
</hibernate-mapping>