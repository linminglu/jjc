<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class 
    name="com.apps.model.UserTradeDetail" 
    table="user_trade_detail"
>
	<cache usage="read-write" /> 
    <id
        name="tradeDetailId"
        type="java.lang.Integer"
        column="trade_detail_id"
    >
        <generator class="identity"/>
    </id>
    <property
        name="userId"
        type="java.lang.Integer"
        column="user_id"
        length="8"
    />
    <property
        name="tradeType"
        type="java.lang.String"
        column="trade_type"
        length="1"
    />
     <property
        name="createTime"
        type="java.util.Date"
        column="create_time"
    />
    <property
        name="cashType"
        type="java.lang.String"
        column="cash_type"
        length="1"
    />
    <property
        name="status"
        type="java.lang.String"
        column="status"
        length="1"
    />
    <property
        name="cashMoney"
        column="cash_money"
        type="java.math.BigDecimal"
        />
     <property
        name="refId"
        type="java.lang.Integer"
        column="ref_id"
         length="8"
    />
    
    <property
        name="userMoney"
        column="user_money"
        type="java.math.BigDecimal"
        />
    <property
        name="remark"
        type="java.lang.String"
        column="remark"
        length="11"
    />
    <property
        name="modelType"
        type="java.lang.String"
        column="model_type"
        length="2"
    />
	<property
        name="sessionNo"
        type="java.lang.String"
        column="session_no"
        length="13"
    />
    <property
        name="type"
        type="java.lang.String"
        column="type"
        length="2"
    />
    <property
        name="loginName"
        type="java.lang.String"
        column="login_name"
        length="50"
    />
    <property
        name="gfxy"
        type="java.lang.String"
        column="gfxy"
        length="1"
    />
</class>
</hibernate-mapping>
