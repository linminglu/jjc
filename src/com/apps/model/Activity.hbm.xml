<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class 
    name="com.apps.model.Activity" 
    table="activity"
>
	<cache usage="read-write" /> 
    <id
        name="id"
        type="java.lang.Integer"
        column="id"
    >
        <generator class="identity"/>
    </id>
     <property
        name="title"
        type="java.lang.String"
        column="title"
        length="200"
    />
    <property
        name="showImg"
        type="java.lang.String"
        column="show_img"
        length="200"
    />
    <property
        name="hideImg"
        type="java.lang.String"
        column="hide_img"
        length="200"
    />
    <property
        name="activityTime"
        type="java.util.Date"
        column="activity_time"
        length="200"
    />
         <property
        name="status"
        type="java.lang.String"
        column="status"
        length="1"
    />
    
</class>
</hibernate-mapping>
