<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
	"http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<!-- ==================================== Data Sources      -->
  	<data-sources>
  	</data-sources>
  	<!-- ==================================== Form Beans        -->
  	<form-beans>
		<form-bean name="fiveForm" type="com.xy.ssc.five.web.form.FiveForm"/>
  	</form-beans>
  	<!-- ==================================== Global Exceptions -->
  	<global-exceptions>
		<exception key="error" type="java.lang.Exception" handler="com.ram.exception.ActionExceptionHandler" />
  	</global-exceptions>
  	<!-- ==================================== Global Forwards    -->
  	<global-forwards>
	 	<forward name="exception" path="/_exception.jsp" />
  	</global-forwards>
  	<!-- ==================================== Action Mappings    -->
  	<action-mappings>
  		<action 
			path="/fiveAction" 
    		type="com.xy.ssc.five.web.action.FiveAction" 
    		name="fiveForm" 
    		scope="request" 
    		parameter="method"
    		validate="false"
    	>
	   	<forward name="init" path="/fiveOpenSessionList.jsp"/>    
	   	<forward name="betList" path="/fiveBetList.jsp"/> 
	   	<forward name="betManager" path="/fiveBetManager.jsp"/>
	   	<forward name="betDetailManager" path="/fiveBetDetailManager.jsp"/>  
	   	</action>
    </action-mappings>
</struts-config>
