<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class 
    name="com.card.model.RechargeWay" 
    table="recharge_way"
>
	<cache usage="read-write" /> 
    <id
        name="id"
        type="java.lang.Integer"
        column="id"
    >
        <generator class="identity"/>
    </id>

    <property
        name="parentId"
        type="java.lang.Integer"
        column="parent_id"
    />
    <property
        name="type"
        type="java.lang.String"
        column="type"
    />
    <property
        name="channelType"
        type="java.lang.String"
        column="channel_type"
    />
    <property
        name="title"
        type="java.lang.String"
        column="title"
    />
    <property
        name="status"
        type="java.lang.String"
        column="status"
    />
    <property
        name="bankName"
        type="java.lang.String"
        column="bank_name"
    />
    <property
        name="bankAccount"
        type="java.lang.String"
        column="bank_account"
    />
    <property
        name="userName"
        type="java.lang.String"
        column="user_name"
    />
    <property
        name="des"
        type="java.lang.String"
        column="des"
    />
    <property
        name="img"
        type="java.lang.String"
        column="img"
    />
    <property
        name="url"
        type="java.lang.String"
        column="url"
    />
</class>
</hibernate-mapping>