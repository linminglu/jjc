<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.xy.lucky28.bjlu28.model.BjLu28GaBet" table="xy_bj_lu28_ga_bet">
		<cache usage="read-write" />
		<id name="betId" type="java.lang.Integer" column="bet_id">
			<generator class="identity" />
		</id>
       <property name="userId" type="java.lang.Integer" column="user_id" length="8" />
       <property name="sessionId" type="java.lang.Integer" column="session_id" length="11" />
       <property name="totalNum" type="java.lang.Integer" column="total_num" length="3" />
       <property name="totalPoint" type="java.math.BigDecimal" column="total_point" length="10" />
       <property name="winCash" type="java.math.BigDecimal" column="win_cash" length="10" />
       <property name="betTime" type="java.util.Date" column="bet_time" />
       <property name="bigSmaOddDual" type="java.lang.Integer" column="big_sma_odd_dual" length="3"></property>
       <property name="combination" type="java.lang.Integer" column="combination" length="3"></property>
       <property name="extreme" type="java.lang.Integer" column="extreme" length="3"></property>
       <property name="code" type="java.lang.Integer" column="code" length="3"></property>
       <property name="color" type="java.lang.Integer" column="color" length="3"></property>
       <property name="leopard" type="java.lang.Integer" column="leopard" length="3"></property>
       <property name="sessionNo" type="java.lang.String" column="session_no" length="11" />

	</class>
</hibernate-mapping>