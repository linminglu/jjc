<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.xy.lucky28.bjlu28.model.BjLu28GaTrend" table="xy_bj_lu28_ga_trend">
		<cache usage="read-write" />
		<id name="trendId" type="java.lang.Integer" column="trend_id">
			<generator class="identity" />
		</id>
       <property name="trendTitle" type="java.lang.String" column="trend_title" length="20" />
       <property name="trendCount" type="java.lang.Integer" column="trend_count" length="5" />
	</class>
</hibernate-mapping>