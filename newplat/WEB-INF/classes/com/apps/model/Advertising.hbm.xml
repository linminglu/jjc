<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class 
    name="com.apps.model.Advertising" 
    table="advertising"
>
	<cache usage="read-write" /> 
    <id
        name="id"
        type="java.lang.Integer"
        column="id"
    >
        <generator class="identity"/>
    </id>
    <property
        name="title"
        type="java.lang.String"
        column="title"
        length="100"
    />
    <property
        name="status"
        type="java.lang.String"
        column="status"
        length="1"
    />
     <property
        name="createDate"
        type="java.util.Date"
        column="create_date"
    />
    <property
        name="adType"
        type="java.lang.String"
        column="ad_type"
        length="1"
    />
    <property
        name="tid"
        column="t_id"
        type="java.lang.Integer"
        length="8"/>
     <property
        name="type"
        type="java.lang.String"
        column="type"
         length="1"
    />
     <property
        name="link"
        type="java.lang.String"
        column="link"
         length="200"
    />
     <property
        name="img"
        type="java.lang.String"
        column="img"
         length="200"
    />
     <property
        name="sort"
        type="java.lang.String"
        column="sort"
         length="2"
    />
     <property
        name="cid"
        type="java.lang.Integer"
        column="c_id"
         length="8"
    />
    <property
        name="busaid"
        type="java.lang.Integer"
        column="busa_id"
         length="8"
    />
    <property
        name="ccid"
        type="java.lang.Integer"
        column="cc_id"
         length="8"
    />
    <property
        name="content"
        type="java.lang.String"
        column="content"
         length="2000"
    />
</class>
</hibernate-mapping>
