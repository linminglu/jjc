<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class 
    name="com.apps.model.Promotion" 
    table="promotion"
>
	<cache usage="read-write" /> 
    <id
        name="id"
        type="java.lang.Integer"
        column="id"
    >
        <generator class="identity"/>
    </id>
    <property
        name="title"
        type="java.lang.String"
        column="title"
    />
    <property
        name="status"
        type="java.lang.String"
        column="status"
    />
    <property
        name="img"
        type="java.lang.String"
        column="img"
    />
    <property
        name="sort"
        type="java.lang.String"
        column="sort"
    />
    <property
        name="content"
        type="java.lang.String"
        column="content"
    />
    <property
        name="startDate"
        type="java.util.Date"
        column="start_date"
    />
    <property
        name="createDate"
        type="java.util.Date"
        column="create_date"
    />
</class>
</hibernate-mapping>
