<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.ram.model.NewsCategoryRl" table="news_category_rl">
		<cache usage="read-write" />
		<id name="rid" type="java.lang.Integer" column="r_id">
			<generator class="identity" />
		</id>
       <property name="cid" type="java.lang.Integer" column="c_id" length="8" />
       <property name="nid" type="java.lang.Integer" column="n_id" length="8" />
	</class>
</hibernate-mapping>